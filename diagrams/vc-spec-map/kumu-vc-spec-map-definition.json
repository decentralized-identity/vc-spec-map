      "_id": "pers-juiNKbbA",
      "name": "Full Map",
      "style": "@settings {\n  template: systems;\n  element-color: categorize(\"Element Type\", olympic);\n  layout-preset: dense;\n  same-label: \"\";\n  opposite-label: \"\";\n}\n\n"
